<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"  xmlns:tools="http://schemas.android.com/tools"  android:layout_height="match_parent"  android:layout_width="match_parent"  android:background="@color/primary"  android:paddingLeft="20dp"  android:paddingRight="20dp"  android:paddingTop="16dp"  android:paddingBottom="16dp"  android:layout_gravity="center"  android:orientation="vertical"  tools:context=".Login">  <ImageView    android:layout_width="@dimen/_170sdp"    android:layout_height="@dimen/_155sdp"    android:src="@drawable/logo_app"    android:layout_centerHorizontal="true"    android:layout_marginTop="@dimen/_50sdp">  </ImageView>  <TextView    android:layout_width="wrap_content"    android:layout_height="wrap_content"    android:layout_centerHorizontal="true"    android:layout_marginTop="@dimen/_200sdp"    android:textStyle="bold"    android:textSize="@dimen/_27ssp"    android:text="POSYANDU"    android:textColor="@color/white" >  </TextView>  <EditText    android:id="@+id/uName"    android:layout_width="@dimen/_246sdp"    android:layout_height="@dimen/_29sdp"    android:layout_marginTop="@dimen/_255sdp"    android:layout_marginEnd="@dimen/_15sdp"    android:paddingLeft="@dimen/_10sdp"    android:hint="Telepon atau Email"    android:background="@drawable/rounded_bg"    android:layout_centerHorizontal="true"    android:singleLine="true"    android:textColorHint="@color/black" />  <EditText    android:id="@+id/passwords"    android:layout_width="@dimen/_246sdp"    android:layout_height="@dimen/_29sdp"    android:paddingLeft="@dimen/_10sdp"    android:layout_marginTop="@dimen/_290sdp"    android:layout_centerHorizontal="true"    android:background="@drawable/rounded_bg"    android:hint="Kata Sandi"    android:inputType="textPassword"    android:singleLine="true"    android:textColorHint="@color/black" />  <Button    android:id="@+id/btnLogin"    android:layout_width="@dimen/_246sdp"    android:layout_height="wrap_content"    android:layout_marginTop="@dimen/_335sdp"    android:backgroundTint="@color/purple_700"    android:textSize="@dimen/_13ssp"    android:text="Sign In"    android:layout_centerHorizontal="true"    android:textColor="@color/white"    android:textAllCaps="true"    />  <Button    android:id="@+id/btnToRegist"    android:layout_width="@dimen/_250sdp"    android:layout_height="wrap_content"    android:layout_marginTop="@dimen/_370sdp"    android:backgroundTint="#2448bd"    android:onClick="RegistrasiView"    android:textSize="15dp"    android:text="Register"    android:layout_centerHorizontal="true"    android:textColor="@color/white"    android:textAllCaps="true"    /></RelativeLayout>