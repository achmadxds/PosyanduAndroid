<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"  xmlns:tools="http://schemas.android.com/tools"  android:layout_height="match_parent"  android:layout_width="match_parent"  xmlns:app="http://schemas.android.com/apk/res-auto"  android:background="@drawable/bgg_reg_antrian"  android:paddingLeft="@dimen/_10sdp"  android:paddingRight="@dimen/_10sdp"  android:paddingTop="@dimen/_17sdp"  android:paddingBottom="@dimen/_17sdp"  android:layout_gravity="center"  android:orientation="vertical"  tools:context=".Login">  <ImageView    android:layout_width="@dimen/_270sdp"    android:layout_height="@dimen/_115sdp"    android:layout_marginTop="@dimen/_30sdp"    android:layout_centerHorizontal="true"    android:foregroundGravity="center"    android:background="@drawable/heading"    />  <GridLayout    android:id="@+id/mainGrids"    android:layout_centerHorizontal="true"    android:layout_centerVertical="true"    android:layout_width="match_parent"    android:layout_height="@dimen/_600sdp"    android:paddingTop="@dimen/_60sdp"    android:paddingBottom="@dimen/_10sdp"    android:columnCount="1"    android:rowCount="2"    tools:ignore="MissingConstraints">    <androidx.cardview.widget.CardView      android:layout_width="match_parent"      android:layout_height="@dimen/_30sdp"      android:layout_row="0"      android:layout_rowWeight="1"      android:layout_column="0"      android:layout_columnWeight="1"      android:layout_gravity="fill"      android:layout_marginTop="@dimen/_200sdp"      app:cardCornerRadius="@dimen/_10sdp"      app:cardElevation="@dimen/_5sdp"      app:cardUseCompatPadding="true"      tools:ignore="OnClick">      <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="center_horizontal"        android:gravity="center"        android:orientation="vertical">        <TextView          android:text="Username"          android:layout_gravity="left"          android:textColor="@color/black"          android:textSize="@dimen/_18ssp"          android:textStyle="bold"          android:layout_marginTop="@dimen/_30sdp"          android:layout_width="@dimen/_100sdp"          android:layout_height="@dimen/_22sdp" />        <EditText          android:id="@+id/uName"          android:layout_width="@dimen/_246sdp"          android:layout_height="@dimen/_29sdp"          android:layout_marginTop="@dimen/_4sdp"          android:paddingLeft="@dimen/_10sdp"          android:hint="Masukan Username Kamu.."          android:background="@drawable/rounded_bg"          android:layout_centerHorizontal="true"          android:singleLine="true"          android:textColorHint="@color/black" />        <TextView          android:text="Password"          android:layout_gravity="left"          android:textColor="@color/black"          android:textSize="@dimen/_18ssp"          android:textStyle="bold"          android:layout_marginTop="@dimen/_12sdp"          android:layout_width="@dimen/_100sdp"          android:layout_height="@dimen/_22sdp" />        <EditText          android:id="@+id/passwords"          android:layout_width="@dimen/_246sdp"          android:layout_height="@dimen/_29sdp"          android:layout_marginTop="@dimen/_4sdp"          android:paddingLeft="@dimen/_10sdp"          android:hint="Masukan Password Kamu.."          android:inputType="textPassword"          android:background="@drawable/rounded_bg"          android:layout_centerHorizontal="true"          android:singleLine="true"          android:textColorHint="@color/black" />          <Button            android:id="@+id/btnLogin"            android:layout_width="@dimen/_146sdp"            android:layout_height="wrap_content"            android:layout_marginTop="@dimen/_12sdp"            android:backgroundTint="#188fcc"            android:textSize="@dimen/_13ssp"            android:text="Sign In"            android:layout_centerHorizontal="true"            android:textColor="@color/white"            android:textAllCaps="true"          />          <Button            android:id="@+id/btnToRegist"            android:layout_width="@dimen/_146sdp"            android:layout_height="wrap_content"            android:layout_marginTop="@dimen/_3sdp"            android:backgroundTint="#353738"            android:onClick="RegistrasiView"            android:textSize="15dp"            android:text="Register"            android:layout_centerHorizontal="true"            android:textColor="@color/white"            android:textAllCaps="true"            />      </LinearLayout>    </androidx.cardview.widget.CardView>  </GridLayout></RelativeLayout>